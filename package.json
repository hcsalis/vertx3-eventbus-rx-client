{
  "name": "vertx3-eventbus-rx-client",
  "version": "0.0.0",
  "description": "vertx3-eventbus-rx-client",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hcsalis/vertx3-eventbus-rx-client.git"
  },
  "author": {
    "name": "Halil Can Salis"
  },
  "keywords": [
    "websockets",
    "websocket",
    "sockjs",
    "vert.x",
    "vertx3-eventbus-client",
    "rxjs",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive 'spec/**/*.spec.ts'",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'spec/**/*.spec.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run spec -- --watch"
  },
  "dependencies": {
    "sockjs-client": "1.1.4",
    "vertx3-eventbus-client": "3.4.1"
  },
  "peerDependencies": {
    "rxjs": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/mocha": "^2.0.0",
    "@types/node": "^7.0.0",
    "chai": "^3.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0",
    "coveralls": "^2.0.0",
    "rxjs": "^5.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "dist"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
